// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgres"
    url      = env("DATABASE_URL")
}

model Engines {
    id          Int    @id @default(autoincrement())
    fieldTag    String
    equipament  String
    description String
    nominal     Float
    power       Float
    rotation    Int
    horsePw     Int

    @@map("engines")
}

